   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "a = 10\n",
    "n = 1\n",
    "x = np.linspace(0, 30, 100)\n",
    "y = n / (np.pi * n**2 + (x - a)**2)\n",
    "\n",
    "plt.plot(x, y)\n",
    "plt.xlabel('x')\n",
    "plt.ylabel('Lorentz Curve')\n",
    "plt.title('Plot of Lorentz Curve')\n",
    "plt.grid(True)\n",
    "plt.show()\n"
   ]
  }
